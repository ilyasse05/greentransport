  <access origin="*"/>
  <allow-intent href="http://greentransport.ma/*" />
  <allow-intent href="https://greentransport.ma/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <allow-navigation href="https://greentransport.ma/*" />
    <allow-navigation href="http://greentransport.ma/*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <allow-navigation href="https://greentransport.ma/*" />
    <allow-navigation href="http://greentransport.ma/*" />
  </platform>
